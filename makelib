cp ./dist/setting/bundles/*.* ./src/assets/dist/.
ng build support
cp ./dist/support/bundles/*.* ./src/assets/dist/.
ng build task
cp ./dist/task/bundles/*.* ./src/assets/dist/.
ng build user
cp ./dist/user/bundles/*.* ./src/assets/dist/.
